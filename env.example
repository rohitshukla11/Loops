NEXT_PUBLIC_CONTRACT_ID=memory-anchor.testnet

# IPFS Configuration
NEXT_PUBLIC_IPFS_GATEWAY=https://ipfs.io/ipfs/

# Web3.Storage (optional, for redundancy)
NEXT_PUBLIC_WEB3_STORAGE_TOKEN=your_web3_storage_token_here

# Golem Base Configuration (Decentralized Data Storage)
NEXT_PUBLIC_GOLEM_PRIVATE_KEY=
